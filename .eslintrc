{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "es6": true
    },
    "rules": {
        "indent": ["error", 4],
        "quotes": ["error", "single"],
        "linebreak-style": ["error", "unix"]
    },
    "extends": "react-app"
}
